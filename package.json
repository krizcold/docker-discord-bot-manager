{
  "name": "discord-bot-manager",
  "version": "1.0.0",
  "description": "Multi-bot manager with Docker-in-Docker support",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && cp -r src/webui/public dist/webui/public",
    "start": "node dist/index.js",
    "docker:build": "docker build -t discord-bot-manager .",
    "docker:run": "docker-compose up -d"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "simple-git": "^3.22.0",
    "uuid": "^9.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3"
  }
}
